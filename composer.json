{
    "name": "diepdpkaiyouit/coupon",
    "description": "A reusable library for managing coupon codes, points, and events",
    "keywords": ["coupon", "library", "php", "laravel"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Coupon\\": "src/"
        }
    },
    "authors": [
        {
            "name": "diepdp",
            "email": "diepdp@kaiyouit.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/database": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Coupon\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Coupon\\Providers\\CouponServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
